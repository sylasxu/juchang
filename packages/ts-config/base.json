{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
      "target": "ESNext",
      "composite": true,
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx"
    }
  }